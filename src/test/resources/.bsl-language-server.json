{
  "language": "en",
  "codeLens": {
    "showCognitiveComplexity": true,
    "showCyclomaticComplexity": true
  },
  "diagnostics": {
    "computeTrigger": "onType",
    "skipSupport": "withSupportLocked",
    "mode": "only",
    "rules": {
      "LineLength": {
        "maxLineLength": 140
      },
      "MethodSize": false
    }
  },
  "traceLog": "build/.trace.log",
  "configurationRoot": "src/test/resources/metadata"
}
