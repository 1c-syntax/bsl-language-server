Функция ФорматироватьЗначениеПоказателя(Показатель)	
	Если Показатель <> Неопределено Тогда 
		Если ТипЗнч(Показатель.Значение) = Тип("Число") Тогда
			
			СписокФорматов = Новый Соответствие();
			СписокФорматов.Вставить("ЧДЦ", Строка(УровеньТочности));
			
			Если НЕ Показатель.НеМасштабируется Тогда
				СписокФорматов.Вставить("ЧС", 0);
				Значение = Показатель.Значение / ВернутьЗначениеДелителя();
			Иначе
				Значение = Показатель.Значение;
			КонецЕсли;
			
			Возврат Формат(Значение, ПреобразоватьВСтрокуФормата(СписокФорматов));
		Иначе
			Возврат Показатель.Значение;
		КонецЕсли;
	КонецЕсли;
КонецФункции

Функция ПолучитьПредставлениеГраницы(Граница)
	ТипЛинии = Граница.attributes.getNamedItem("ТипЛинии").value;
	HTMLТипЛинии = "";
	Если ТипЛинии = "Сплошная" Тогда
		HTMLТипЛинии = "solid";
	ИначеЕсли ТипЛинии = "Точечная" Тогда
		HTMLТипЛинии = "dotted";
	ИначеЕсли ТипЛинии = "Двойная" Тогда
		HTMLТипЛинии = "double";
	ИначеЕсли ТипЛинии = "РедкийПунктир" Или ТипЛинии = "ЧастыйПунктир" Или ТипЛинии = "БольшойПунктир" Тогда
		HTMLТипЛинии = "dashed";
	КонецЕсли;
	Возврат "black " + Граница.attributes.getNamedItem("Толщина").value + "px " + HTMLТипЛинии;
КонецФункции

Функция ПолучитьЦвет(ПредставлениеЦвета)	
	HTMLПредставление = "#";
	СоставляющиеЦвета = ОбщегоНазначенияУХ.РазложитьСтрокуВМассивПодстрок(ПредставлениеЦвета, ",");
	Для Индекс = 0 По 2 Цикл
		HTMLПредставление = HTMLПредставление + ПолучитьШестнадцатиричноеПредставление(Число(СоставляющиеЦвета[Индекс]));
	КонецЦикла;
	Возврат HTMLПредставление;
КонецФункции

Функция ПолучитьШестнадцатиричноеПредставление(Знач Число)
	Строка = "";
	Пока Число >= 16 Цикл
		Дробь = Число / 16;
		Число = Цел(Дробь);
		Строка = ПолучитьШестнадцатиричнуюЦифру(Дробь - Число) + Строка;
	КонецЦикла;	
	Строка = ПолучитьШестнадцатиричнуюЦифру(Число);
	Возврат Строка;
КонецФункции

Функция ПолучитьШестнадцатиричнуюЦифру(Знач Число)
	Если Число < 10 Тогда
		Возврат Строка(Число);
	Иначе
		Возврат Символ(65 + Число - 10);
	КонецЕсли;
КонецФункции

