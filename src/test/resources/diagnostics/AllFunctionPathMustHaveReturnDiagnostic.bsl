Функция СуммаСкидки(Знач КорзинаЗаказа)
    Если КорзинаЗаказа.Строки.Количество() > 10 Тогда
        Возврат Скидки.СкидкаНаКрупнуюКорзину(КорзинаЗаказа);
    ИначеЕсли КорзинаЗаказа.ЕстьКартаЛояльности Тогда
        // функция возвращает непредусмотренное значение Неопределено
        Скидки.СкидкаПоКартеЛояльности(КорзинаЗаказа);
    Иначе
        Возврат 0;
    КонецЕсли;
КонецФункции

Функция ЦиклДляПроверки(Коллекция, Поиск)

    // если assumeLoopsAreExecutedOnce=true - то не будет срабатывания
    // по ветке Для..Цикл -> КонецФункции
    Для Каждого Элемент Из Коллекция Цикл
        Если Элемент = Поиск Тогда
            Возврат 1;
        КонецЕсли;
    КонецЦикла;

КонецФункции

// циклы Пока Истина пропустим, т.к. их
// логика выхода обычно сложнее чем простой проход по условию
// и зависит от контекста тела цикла
Функция НайтиСледующееСовпадение(ТекущиеДанные, Вперед = Истина)

	СтрокаПоискаВРЕГ = ВРЕГ(СокрЛ(СтрокаПоискаФункции));
	СледующаяСтрока = СледующаяСтрокаФункции(ТекущиеДанные, Вперед);

	Пока Истина Цикл
		Если СледующаяСтрока = Неопределено Тогда
			Возврат Неопределено;
		КонецЕсли;

		Точка = СтрНайти(СледующаяСтрока.ИмяДляФормулы, ".");
		Скобка = СтрНайти(СледующаяСтрока.ИмяДляФормулы, "(");
		ИмяФункцииВРЕГ = ВРег(Сред(СледующаяСтрока.ИмяДляФормулы, Точка + 1, Скобка - Точка - 1));

		Если СтрНайти(ИмяФункцииВРЕГ, СтрокаПоискаВРЕГ) > 0 Тогда
			Возврат СледующаяСтрока;
		КонецЕсли;

		// данная функция обеспечивает корректный выход
		// в зоне нашей функции мы не можем сказать, будет ли выход из цикла во всех ветках
		СледующаяСтрока = СледующаяСтрокаФункции(СледующаяСтрока, Вперед);

	КонецЦикла;

КонецФункции

