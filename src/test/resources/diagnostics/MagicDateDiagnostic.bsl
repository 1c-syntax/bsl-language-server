Процедура ПроверкаСтруктурСДатами()
    // Операция Вставить - НЕ должно быть ошибки, так как имя переменной - это ключ структуры
    НоваяСтруктура = Новый Структура;
    НоваяСтруктура.Вставить("ДатаНачала", '20250101'); // Нет ошибки
    НоваяСтруктура.Вставить("ДатаОкончания", '20251231'); // Нет ошибки
    НоваяСтруктура.Вставить("МаксимальнаяДата", '39991231235959'); // Нет ошибки

    // Конструктор структуры - НЕ должно быть ошибки
    ПроверочнаяСтруктура2 = Новый Структура("ДатаНачала, ДатаОкончания", '20250101', '20251231'); // Нет ошибки
    ПроверочнаяСтруктура3 = Новый Структура("ЕдинственнаяДата", '20230101'); // Нет ошибки
    
    // Прямое присваивание свойству структуры - НЕ должно быть ошибки
    СтруктураСПолями = Новый Структура("ДатаНачала, ДатаОкончания");
    СтруктураСПолями.ДатаНачала = '20250101'; // Нет ошибки
    СтруктураСПолями.ДатаОкончания = '20251231'; // Нет ошибки
    
    // Фиксированная структура - НЕ должно быть ошибки
    СтруктураПодготовленная = Новый Структура;
    СтруктураПодготовленная.Вставить("Значение", '20240101'); // Нет ошибки
    СтруктураПодготовленная.Вставить("Счетчик", '20250101000000'); // Нет ошибки

    ФиксированнаяСтруктура1 = Новый ФиксированнаяСтруктура(СтруктураПодготовленная); // Нет ошибки
    ФиксированнаяСтруктура2 = Новый ФиксированнаяСтруктура("Значение", '20240101'); // Нет ошибки

    // Соответствие - НЕ должно быть ошибки
    СоответствиеКодов = Новый Соответствие;
    СоответствиеКодов.Вставить("Код", '20230101'); // Нет ошибки
    СоответствиеКодов.Вставить("Номер", '20240101'); // Нет ошибки
    СоответствиеКодов.Вставить("Идентификатор", '20250101'); // Нет ошибки
    
    СобытияДаты = Новый Соответствие;
    СобытияДаты.Вставить('19800101', "Олимпиада в Москве"); // Нет ошибки
    СобытияДаты.Вставить('19610412', "Первый полет человека в космос"); // Нет ошибки

    // Тест для покрытия случая, когда первый параметр - переменная или функция
    ПеременнаяКлючДата = "ДатаКлюч";
    СтруктураПеременнаяДата = Новый Структура;
    СтруктураПеременнаяДата.Вставить(ПеременнаяКлючДата, '20250101'); // Нет ошибки - первый параметр переменная
    СтруктураПеременнаяДата.Вставить(ПолучитьКлючДаты(), '20251231'); // Нет ошибки - первый параметр функция

КонецПроцедуры

